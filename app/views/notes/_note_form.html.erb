      
<%= form_for @note, :url=>{:action=>"update"} do |f|%>
   <%= f.hidden_field :note_type_id, :value => 2%>
   <p>
      <%= f.label "Notes:" %><br>
      <%= f.text_area :content, :cols => "30", :rows => "6"%>
   </p>
   <div id= "buttonId" class="actions">
      <%= f.submit "Submit", class:"btn btn-primary btn-padded" %>
   </div>
<% end %> 

              