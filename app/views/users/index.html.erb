<div class="container">
   <div class="row">
      <br/>
      <%= link_to 'New User', users_new_user_path ,class:"btn btn-primary" %> <br/><br/>
   <h3>Users</h3>
      <div class="column">
      <table class="table">
         <tbody>
            <% @users.each do |user| %>
            <tr>
               <%= render user %>
            </tr>
            <% end %>
         </tbody>
      </table>
      </div>
   </div>
</div>
