<%= form_for @group, :url=>{:action=>"update"} do |f|%>
  <p>
    <%= f.label :name, "Organization Name" %><br>
    <%= f.text_field :name %>
  </p>
   <p>
    <%= f.label :description %><br>
    <%= f.text_area :description, :cols => "30", :rows => "6"%>
  </p>
  <div id= "buttonId" class="actions">
    <%= f.submit "Submit", class:"btn btn-primary btn-padded" %>
  </div>
<% end %>
